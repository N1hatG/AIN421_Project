{
  "model": "ANFIS-x7 (1D Gaussian TS-first-order)",
  "train_csv": "data/splits/anfis/anfis_iter1_train.csv",
  "test_csv": "data/splits/anfis/anfis_iter1_test.csv",
  "epochs": 120,
  "lr": 0.05,
  "ridge": 1e-06,
  "seed": 42111,
  "mf_count": 5,
  "scale": true,
  "scaler": {
    "x_min": 0.0,
    "x_max": 40.0
  },
  "sigma_mult": 1.0,
  "thresholds": "optimize",
  "early_stopping": {
    "enabled": true,
    "patience": 15,
    "min_delta": 0.0005,
    "monitor": "mse"
  }
}